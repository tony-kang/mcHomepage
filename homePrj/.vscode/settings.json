{
  "svelte.plugin.svelte.format.enable": true,
  "svelte.plugin.svelte.diagnostics.enable": true,
  "svelte.plugin.svelte.hover.enable": true,
  "svelte.plugin.svelte.completions.enable": true,
  "svelte.plugin.svelte.documentSymbols.enable": true,
  "svelte.plugin.svelte.codeActions.enable": true,
  "svelte.plugin.svelte.rename.enable": true,
  "svelte.plugin.svelte.selectionRange.enable": true,
  "svelte.plugin.svelte.foldingRange.enable": true,
  "svelte.plugin.svelte.linkedEditing.enable": true,
  "svelte.plugin.svelte.signatureHelp.enable": true,
  "svelte.plugin.svelte.inlayHints.enable": true,
  "svelte.plugin.svelte.diagnostics.config": {
    "a11y-click-events-have-key-events": "ignore",
